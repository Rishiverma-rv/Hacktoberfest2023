class Solution {
public:
    string reverseWords(string s) {
        string ans="";
        string temp="";
        int n=s.size();
        for(int i=0;i<n;i++){
            
            if(s[i]!=' ')
            {
                temp+=s[i];
            }
            else
            {
                reverse(temp.begin(),temp.end());
                temp+=" ";
                ans+=temp;
                temp=""; 
            }
        }
        reverse(temp.begin(),temp.end());
        ans+=temp;
        return ans;
        
    }
};
